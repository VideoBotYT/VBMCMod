{
	"schemaVersion": 1,
	"id": "rubymod",
	"version": "${version}",
	"name": "RubyMod",
	"description": "Mod made by following a course by Kaupenjoe",
	"authors": [
		"VideoBot",
		"Kaupenjoe"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/rubymod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.videobot.rubymod.RubyMod"
		],
		"fabric-datagen": [
			"net.videobot.rubymod.RubyModDataGenerator"
		],
		"client": [
			"net.videobot.rubymod.RubyModClient"
		]
	},
	"mixins": [
		"rubymod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}